{
    "Logging": {
        "Region": "us-east-2",
        "LogGroup": "helios-integ-task-api",
        "IncludeLogLevel": true,
        "IncludeCategory": true,
        "IncludeNewline": true,
        "IncludeException": true,
        "IncludeEventId": false,
        "IncludeScopes": false,

        "Console": {
            "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] ",
            "UseUtcTimestamp": true
        },
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"

        }
    }
}
