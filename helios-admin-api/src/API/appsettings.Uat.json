{
  "Logging": {
    "Region": "us-east-2",
    "LogGroup": "helios-uat-admin-api",
    "IncludeLogLevel": true,
    "IncludeCategory": true,
    "IncludeNewline": true,
    "IncludeException": true,
    "IncludeEventId": false,
    "IncludeScopes": false,

    "Console": {
      "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] ",
      "UseUtcTimestamp": true
    },


    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ExportTenantVersion": "0.1",
  "AWS": {
    "AWS_TMP_BUCKET_NAME": "sunny-tmp-uat"
  },
  "UserAPI": "http://user-api.helios-uat.svc.cluster.local/api/v1/",
  "WalletAPI": "http://wallet-api.helios-uat.svc.cluster.local/api/v1/",
  "TaskAPI": "http://task-api.helios-uat.svc.cluster.local/api/v1/",
  "TenantAPI": "http://tenant-api.helios-uat.svc.cluster.local/api/v1/",
  "CohortAPI": "http://cohort-api.helios-uat.svc.cluster.local/api/v1/",
  "FisAPI": "http://fis-api.helios-uat.svc.cluster.local/api/v1/",
  "SweepstakesAPI": "http://sweepstakes-api.helios-uat.svc.cluster.local/api/v1/",
  "CmsAPI": "http://cms-api.helios-uat.svc.cluster.local/api/v1/",
  "HealthAPI": "http://health-api.helios-uat.svc.cluster.local/api/v1/"
}
