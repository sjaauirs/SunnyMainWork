{
  "Logging": {
    "Region": "us-east-2",
    "LogGroup": "helios-sbx-admin-api-log-group",
    "IncludeLogLevel": true,
    "IncludeCategory": true,
    "IncludeNewline": true,
    "IncludeException": true,
    "IncludeEventId": false,
    "IncludeScopes": false,

    "Console": {
      "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] ",
      "UseUtcTimestamp": true
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ExportTenantVersion": "0.1",
  "AWS": {
    "AWS_TMP_BUCKET_NAME": "helios-sbx-tmp-s3"
  },

  "UserAPI": "http://user-api.helios-sbx.svc.cluster.local/api/v1/",
  "WalletAPI": "http://wallet-api.helios-sbx.svc.cluster.local/api/v1/",
  "TaskAPI": "http://task-api.helios-sbx.svc.cluster.local/api/v1/",
  "TenantAPI": "http://tenant-api.helios-sbx.svc.cluster.local/api/v1/",
  "CohortAPI": "http://cohort-api.helios-sbx.svc.cluster.local/api/v1/",
  "FisAPI": "http://fis-api.helios-sbx.svc.cluster.local/api/v1/",
  "SweepstakesAPI": "http://sweepstakes-api.helios-sbx.svc.cluster.local/api/v1/",
  "CmsAPI": "http://cms-api.helios-sbx.svc.cluster.local/api/v1/",
  "HealthAPI": "http://health-api.helios-sbx.svc.cluster.local/api/v1/"
}
