{
    "Logging": {
        "Region": "us-east-2",
        "LogGroup": "helios-dev-admin-api",
        "IncludeLogLevel": true,
        "IncludeCategory": true,
        "IncludeNewline": true,
        "IncludeException": true,
        "IncludeEventId": false,
        "IncludeScopes": false,

        "Console": {
            "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] ",
            "UseUtcTimestamp": true
        },


        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "ExportTenantVersion": "0.1",
    "AWS": {
        "AWS_TMP_BUCKET_NAME": "sunny-tmp-dev"
    },

    "UserAPI": "http://user-api.user-api.svc.cluster.local/api/v1/",
    "WalletAPI": "http://wallet-api.wallet-api.svc.cluster.local/api/v1/",
    "TaskAPI": "http://task-api.task-api.svc.cluster.local/api/v1/",
    "TenantAPI": "http://tenant-api.tenant-api.svc.cluster.local/api/v1/",
    "CohortAPI": "http://cohort-api.cohort-api.svc.cluster.local/api/v1/",
    "FisAPI": "http://fis-api.fis-api.svc.cluster.local/api/v1/",
    "SweepstakesAPI": "http://sweepstakes-api.sweepstakes-api.svc.cluster.local/api/v1/",
    "CmsAPI": "http://cms-api.cms-api.svc.cluster.local/api/v1/",
    "HealthAPI": "http://health-api.health-api.svc.cluster.local/api/v1/"

}