{
  "Logging": {
    "Region": "us-east-2",
    "LogGroup": "helios-newdev-user-api",
    "IncludeLogLevel": true,
    "IncludeCategory": true,
    "IncludeNewline": true,
    "IncludeException": true,
    "IncludeEventId": false,
    "IncludeScopes": false,

    "Console": {
      "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] ",
      "UseUtcTimestamp": true
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "TenantAPI": "http://tenant-api.helios-dev.svc.cluster.local/api/v1/",
  "AdminAPI": "http://admin-api.helios-dev.svc.cluster.local/api/v1/"
}