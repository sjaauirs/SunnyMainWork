{
  "Logging": {
    "Region": "us-east-2",
    "LogGroup": "helios-sbx-user-api-log-group",
    "IncludeLogLevel": true,
    "IncludeCategory": true,
    "IncludeNewline": true,
    "IncludeException": true,
    "IncludeEventId": false,
    "IncludeScopes": false,

    "Console": {
      "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] ",
      "UseUtcTimestamp": true
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AWS": {
    "AWS_PUBLIC_BUCKET_NAME": "helios-sbx-public"
  },
  "TenantAPI": "http://tenant-api.helios-sbx.svc.cluster.local/api/v1/",
  "AdminAPI": "http://admin-api.sbx.sunnyrewards.com/api/v1/"
}
